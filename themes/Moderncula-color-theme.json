{
  "name": "Moderncula",
  "type": "dark",
  "colors": {
    "activityBar.background": "#21252b",
    "editor.background": "#282c34",
    "editor.foreground": "#abb2bf",
    "editor.lineHighlightBackground": "#99bbff0a",
    "editor.selectionBackground": "#007acc",
    "editorGroupHeader.tabsBackground": "#21252b",
    "editorIndentGuide.activeBackground": "#5c626a",
    "editorLineNumber.activeForeground": "#abb2bf",
    "editorWarning.foreground": "#ffc66d",
    "list.activeSelectionBackground": "#007acc",
    "list.activeSelectionForeground": "#ffffff",
    "list.hoverBackground": "#3a3f4b",
    "list.inactiveSelectionBackground": "#3a3f4b",
    "scrollbar.shadow": "#00000000",
    "sideBar.background": "#21252b",
    "sideBar.foreground": "#abb2bf",
    "tab.inactiveBackground": "#21252b",
    "tab.inactiveForeground": "#9da5b499",
    "titleBar.activeBackground": "#21252b"
  },
  "tokenColors": [
    {
      "name": "Keywords",
      "scope": "keyword, keyword.operator.new, keyword.operator.expression, storage, variable.language.this, constant.language",
      "settings": {
        "foreground": "#cc7832",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Less important keywords",
      "scope": "keyword.operator, punctuation.section.embedded, punctuation.definition.template-expression",
      "settings": {
        "foreground": "#cc7832",
        "fontStyle": ""
      }
    },
    {
      "name": "Class",
      "scope": "entity.name.type, entity.other.inherited-class, support.class.component",
      "settings": {
        "foreground": "#ffc66d"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "name": "Function",
      "scope": "entity.name.function, support.function",
      "settings": {
        "foreground": "#7a9ec2"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#98c379"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#abb2bf"
      }
    },
    {
      "name": "Object property",
      "scope": "variable.other.property, variable.other.object.property, meta.object-literal.key, support.variable.property",
      "settings": {
        "foreground": "#9876aa"
      }
    },
    {
      "name": "JSX prop names",
      "scope": "entity.other.attribute-name",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Resets",
      "scope": "meta.brace.round",
      "settings": {
        "foreground": "#abb2bf",
        "fontStyle": ""
      }
    }
  ]
}
